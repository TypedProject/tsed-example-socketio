    "@tsed/swagger": "5.27.4",
    "@tsed/testing": "5.27.4",
    "@types/swagger-schema-official": "^2.0.9",
    "angular": "1.6.6",
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "consolidate": "^0.15.1",
    "cookie-parser": "^1.4.3",
