    "materialize-css": "^1.0.0",
    "method-override": "^2.3.10",
    "node-uuid": "^1.4.8",
    "pug": "^2.0.3",
    "socket.io": "^2.2.0",
    "ts-json-properties": "1.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.11.1",
    "@types/http-proxy": "^1.16.2",
